<h1>編輯團隊</h1>

<%= simple_form_for(@team,:url => request.env['PATH_INFO'], :method => :post) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, label: "團隊名稱" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "送出", class: "btn btn-primary" %>
    <a href="/home/function/team"><button class= "btn btn-primary">上一頁</button></a>
  </div>
<% end %>
