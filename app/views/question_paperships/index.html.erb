<p id="notice"><%= notice %></p>

<h1>Question Paperships</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @question_paperships.each do |question_papership| %>
      <tr>
        <td><%= question_papership.order %></td>
        <td><%= link_to 'Show', question_papership %></td>
        <td><%= link_to 'Edit', edit_question_papership_path(question_papership) %></td>
        <td><%= link_to 'Destroy', question_papership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Question Papership', new_question_papership_path %>
